{
  "name": "jq-cli-wrapper",
  "version": "1.0.0",
  "description": "A npm wrapper around the jq cli",
  "author": "James Young <jmeyoung@gmail.com> (http://jamsyoung.com/)",
  "main": "",
  "bin": {
    "jq": "./vendor/jq"
  },
  "scripts": {
    "postinstall": "rm -rf vendor; node scripts/link-binary.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamsyoung/jq-cli-wrapper"
  },
  "bugs": {
    "url": "https://github.com/jamsyoung/jq-cli-wrapper/issues"
  },
  "homepage": "https://github.com/jamsyoung/jq-cli-wrapper",
  "engines": {
    "node": ">=8.11.1"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "dependencies": {
    "bin-wrapper": "3.0.2"
  },
  "devDependencies": {},
  "keywords": [
    "cli-wrapper",
    "jq",
    "json"
  ],
  "license": "MIT",
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "printWidth": 100
        }
      }
    ]
  }
}
